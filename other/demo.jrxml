		<queryString>
			<![CDATA[SELECT 
  pmd.barcode as material_code,
  pm.material_name,
  pmd.scaling_factor as material_specification,
	CASE (rom.material_quantity MOD pmd.scaling_factor) WHEN 0 THEN (rom.material_quantity DIV pmd.scaling_factor) ELSE 
		rom.material_quantity
	END as material_quantity,
  CASE (rom.material_quantity MOD pmd.scaling_factor) WHEN 0 THEN pmd.auxiliary_unit ELSE 
	a.auxiliary_unit
	END as auxiliary_unit,
  rom.discount_price,
  rom.discount_price * rom.material_quantity AS total_amount,
CASE (rom.material_quantity MOD pmd.scaling_factor) WHEN 0 THEN concat((rom.material_quantity DIV pmd.scaling_factor),' ',pmd.auxiliary_unit) ELSE 
	concat((rom.material_quantity DIV pmd.scaling_factor),' ',pmd.auxiliary_unit," + ",(rom.material_quantity MOD pmd.scaling_factor),' ',a.auxiliary_unit)
END as remark
FROM
 rep_order_material_$P!{gid} rom 
  LEFT JOIN pub_material_$P!{gid} pm 
    ON pm.material_id = rom.material_id 
  LEFT JOIN pub_material_detail_$P!{gid} pmd 
    ON rom.md_id = pmd.md_id 
  JOIN pub_material_detail_$P!{gid} a  ON rom.material_id = a.material_id
    WHERE rom.order_num = $P{orderid} AND rom.order_type = 101 AND a.scaling_factor = 1
    ORDER BY rom.order_material_id asc]]>
		</queryString>
