	<parameter name="pid" class="java.lang.Long"/>
	<parameter name="gid" class="java.lang.Long"/>
	<parameter name="orderid" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
a.sale_order_id,
a.sale_order_num,
a.delivery_period,
a.expect_receiving_time,
a.cli_phone,
a.cli_address,
a.expect_receiving_amount,
a.discount_amount,
a.actual_receiving_amount,
a.out_time,
a.done_time,
a.platform_id,
a.group_id,
DATE_FORMAT(a.create_time,'%Y-%m-%d') as create_time,
a.create_by,
a.remark,
a.dept_name,
a.employee_name,
a.cli_name,
a.name,
a.pm_name,
a.account_name,
a.invoice_flag,
c.warehouse_addr,
c.warehouse_tel,
d.account_num,
d.account_bank,
e.platform_name,
f.sum_quantity,
f.sum_total_amount,
to_china_num(f.sum_total_amount) as china_money
FROM sal_order_$P!{gid} a
LEFT JOIN pub_warehouse_$P!{gid} c ON a.warehouse_id = c.warehouse_id
LEFT JOIN pub_account_$P!{gid} d ON a.collection_account = d.account_id
LEFT JOIN sys_platform e ON a.platform_id = e.platform_id
left join (
SELECT 
	rom.order_num,
  sum(rom.material_quantity) AS sum_quantity,
  sum(rom.discount_price * rom.material_quantity) AS sum_total_amount
FROM
	rep_order_material_$P!{gid} rom 
  WHERE rom.order_num = $P{orderid} AND rom.order_type = 101
group by rom.order_num
) f on a.sale_order_id = f.order_num
WHERE a.sale_order_id = $P{orderid} AND a.platform_id = $P{pid} AND a.group_id = $P{gid}]]>
	</queryString>
